<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>UkadGroup.EfRepository</PackageId>
    <Authors>Ukad-Group</Authors>
    <Description>Implemenation of generic repository interface for entity framework</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/ukad-group/EfRepository</PackageProjectUrl>
    <RepositoryUrl>https://github.com/ukad-group/EfRepository</RepositoryUrl>
    <PackageReleaseNotes>initial release</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="3.1.6" />
    <PackageReference Include="UkadGroup.IRepository" Version="1.0.1" />
  </ItemGroup>

</Project>
